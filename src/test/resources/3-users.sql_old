CREATE ROLE test2 NOSUPERUSER NOCREATEDB NOCREATEROLE INHERIT LOGIN PASSWORD 'test2';
GRANT CONNECT ON DATABASE object_store_test TO test2;

CREATE ROLE test NOSUPERUSER NOCREATEDB NOCREATEROLE INHERIT LOGIN PASSWORD 'test';;
GRANT ALL PRIVILEGES ON DATABASE object_store_test TO test;

GRANT USAGE, CREATE ON SCHEMA seqdb TO test2;
ALTER DEFAULT PRIVILEGES IN SCHEMA seqdb GRANT ALL PRIVILEGES ON TABLES TO test2;

GRANT USAGE ON SCHEMA seqdb TO test;
GRANT SELECT, INSERT, UPDATE, DELETE, REFERENCES ON ALL TABLES IN SCHEMA seqdb TO test;
GRANT USAGE, SELECT ON ALL SEQUENCES IN SCHEMA seqdb TO test;

ALTER DEFAULT PRIVILEGES FOR USER test2 IN SCHEMA seqdb GRANT SELECT, INSERT, UPDATE, DELETE, REFERENCES ON TABLES TO test;
ALTER DEFAULT PRIVILEGES FOR USER test2 IN SCHEMA seqdb GRANT SELECT, USAGE ON SEQUENCES TO test;
--FOR USER seqdb_migration is not a typo here, it means something like "when user seqdb_migration do something (create), grant the following permissions.
